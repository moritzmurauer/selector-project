IAAI,CAAC,KAJiB,EAKtB,KAAK,CAAC,KALgB,CADL;;AAAA;AACb,gBAAA,GADa;;AAQnB,oBAAI,CAAC,KAAK,CAAC,KAAX,EAAkB;AAChB,kBAAA,MAAM,CAAC,IAAP,CAAY;AAAE,oBAAA,IAAI,EAAE,WAAR;AAAqB,oBAAA,MAAM,EAAE;AAAE,sBAAA,EAAE,EAAE,GAAG,CAAC,IAAJ,CAAS;AAAf;AAA7B,mBAAZ;AACF;;AAVmB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,OAAJ;;AAAA,sBAAX,YAAW;AAAA;AAAA;AAAA,OAAjB;;AAaA,WAAO;AACL,MAAA,KAAK,EAAL,KADK;AAEL,MAAA,QAAQ,EAAR,QAFK;AAGL,MAAA,WAAW,EAAX,WAHK;AAIL,MAAA,YAAY,EAAZ,YAJK;AAKL,MAAA,GAAG,EAAH,GALK;AAML,MAAA,KAAK,EAAL,KANK;AAOL,MAAA,SAAS,EAAT,SAPK;AAQL,MAAA,OAAO,EAAP,OARK;AASL,MAAA,KAAK,EAAL,KATK;AAUL,MAAA,IAAI,EAAJ;AAVK,KAAP;AAYD;AA7CY,CAAf","sourcesContent":["<template>\n  <div class=\"bg\">\n    <div class=\"background-cliff\">\n      <form\n        class=\"mt-4 space-y-12 formblock shadow-md\"\n        @submit.prevent=\"handleSubmit\"\n      >\n        <h3>Sign up</h3>\n        <input\n          class=\"bg-white focus:outline-none focus:shadow-outline border border-gray-300 rounded-lg py-2 pb-3 px-4 block w-full appearance-none leading-normal\"\n          type=\"text\"\n          v-model=\"displayName\"\n          placeholder=\"Name\"\n        />\n        <input\n          class=\"bg-white focus:outline-none focus:shadow-outline border border-gray-300 rounded-lg py-2 pb-3 px-4 block w-full appearance-none leading-normal\"\n          type=\"email\"\n          v-model=\"email\"\n          placeholder=\"Email\"\n        />\n        <input\n          class=\"bg-white focus:outline-none focus:shadow-outline border border-gray-300 rounded-lg py-2 pb-3 px-4 block w-full appearance-none leading-normal\"\n          type=\"password\"\n          v-model=\"password\"\n          placeholder=\"password\"\n        />\n        <div class=\"mt-2 mb-5\">\n          <h4>Please select a category</h4>\n          <select\n            class=\"mb-1 mt-1 pl-3 p-3 pt-1 pb-1 outline-primary\"\n            v-model=\"style\"\n            placeholder=\"pick here\"\n          >\n            <option value=\"\" disabled selected>Select your option</option>\n            <option\n              selected=\"category?\"\n              class=\"p-3\"\n              v-for=\"option in options\"\n              :key=\"option.value\"\n            >\n              {{ option.text }}\n            </option>\n          </select>\n          <p>Category: {{ style }}</p>\n        </div>\n        <div class=\"error\" v-if=\"error\">{{ error }}</div>\n\n        <button class=\"btn outline\" v-if=\"!isPending\">Sign up</button>\n        <button v-if=\"isPending\" disabled>Loading...</button>\n\n        <p class=\"mt-1 d-flex justify-content-center align-center\">\n          Already have an Account? Log in&nbsp;<router-link\n            :to=\"{ name: 'Login' }\"\n            ><span class=\"underline-link\">here</span>\n          </router-link>\n        </p>\n      </form>\n    </div>\n  </div>\n</template>\n\n<script>\nimport useSignup from \"@/composables/useSignup\";\nimport { ref } from \"vue\";\nimport { useRouter } from \"vue-router\";\n\nexport default {\n  setup() {\n    const { error, signup, isPending } = useSignup();\n    const router = useRouter();\n\n    const bio = ref(\"\");\n    const email = ref(\"\");\n    const password = ref(\"\");\n    const displayName = ref(\"\");\n    const grid = ref(\"Triple Grid\");\n\n    // Defines possible Category Inputs\n    const options = ref([\n      { text: \"Photography\", value: \"A\" },\n      { text: \"Drawings\", value: \"B\" },\n      { text: \"Illustration\", value: \"C\" },\n    ]);\n\n    const style = ref(\"\");\n\n    const handleSubmit = async () => {\n      const res = await signup(\n        email.value,\n        password.value,\n        displayName.value,\n        grid.value,\n        style.value\n      );\n      if (!error.value) {\n        router.push({ name: \"UserPosts\", params: { id: res.user.uid } });\n      }\n    };\n\n    return {\n      email,\n      password,\n      displayName,\n      handleSubmit,\n      bio,\n      error,\n      isPending,\n      options,\n      style,\n      grid,\n    };\n  },\n};\n</script>\n\n<style scoped>\n.btn {\n  width: 100%;\n}\n\n.underline-link {\n  text-decoration: underline;\n}\n</style>\n"],"sourceRoot":""}]}