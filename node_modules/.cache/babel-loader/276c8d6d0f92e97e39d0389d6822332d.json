   <div>\n      <div v-if=\"userInfo\">\n        <div class=\"flex items-center justify-center my-div\">\n          <div class=\"text-center mb-12\">\n            <img\n              class=\"w-48 h-48 rounded-full mx-auto object-cover object-center\"\n              v-if=\"userInfo.avatarUrl\"\n              :src=\"userInfo.avatarUrl\"\n            />\n            <h1 class=\"mt-2 text-4xl\">{{ userInfo.displayName }}</h1>\n            <p class=\"mt-2\">{{ userInfo.bio }}</p>\n            <button class=\"btn-full rounded-full mt-4\">\n              <a\n                :href=\"userInfo.website\"\n                target=\"_blank\"\n                rel=\"noopener noreferrer\"\n                >get in touch</a\n              >\n            </button>\n          </div>\n        </div>\n        <div></div>\n      </div>\n      <div v-else>\n        hell no\n      </div>\n    </div>\n\n    <div v-if=\"posts\" class=\"bg-white\">\n      <!-- Grid 1 -->\n      <div v-if=\"userInfo.grid === 'Single Grid'\">\n        <div\n          class=\"grid\nsm:grid-cols-1 md:grid-cols-2 lg:grid-cols-3 xl:grid-cols-1 py-8 px-225px\"\n        >\n          <div class=\"ml-4\" v-for=\"post in posts\" :key=\"post.id\">\n            <router-link :to=\"{ name: 'PostDetail', params: { id: post.id } }\">\n              <div>\n                <img class=\"h-300px w-auto\" :src=\"post.coverUrl\" />\n              </div>\n              <div>\n                <p>{{ post.title }}</p>\n              </div>\n            </router-link>\n          </div>\n        </div>\n      </div>\n\n      <!-- Grid 2 -->\n      <div v-if=\"userInfo.grid === 'Double Grid'\">\n        <div\n          class=\"grid\nsm:grid-cols-1 md:grid-cols-2 lg:grid-cols-3 xl:grid-cols-2 py-8 px-225px\"\n        >\n          <div class=\"ml-4\" v-for=\"post in posts\" :key=\"post.id\">\n            <router-link :to=\"{ name: 'PostDetail', params: { id: post.id } }\">\n              <div>\n                <img class=\"h-300px w-auto\" :src=\"post.coverUrl\" />\n              </div>\n              <div>\n                <p>{{ post.title }}</p>\n              </div>\n            </router-link>\n          </div>\n        </div>\n      </div>\n\n      <!-- Grid 3 -->\n      <div v-if=\"userInfo.grid === 'Triple Grid'\">\n        <div\n          class=\"grid\nsm:grid-cols-1 md:grid-cols-2 lg:grid-cols-3 xl:grid-cols-3 py-8 px-225px\"\n        >\n          <div class=\"ml-4\" v-for=\"post in posts\" :key=\"post.id\">\n            <router-link :to=\"{ name: 'PostDetail', params: { id: post.id } }\">\n              <div>\n                <img class=\"h-300px w-auto\" :src=\"post.coverUrl\" />\n              </div>\n              <div>\n                <p>{{ post.title }}</p>\n              </div>\n            </router-link>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport getUser from \"@/composables/getUser\";\nimport getCollection from \"@/composables/getCollection\";\nimport getDocument from \"@/composables/getDocument\";\nimport { useRoute } from \"vue-router\";\nimport { ref, computed } from \"vue\";\n\nexport default {\n  name: \"UserPosts\",\n  setup() {\n    const route = useRoute();\n    const user = ref(getUser().user);\n    const userInfo = ref(getDocument(\"users\", route.params.id).document);\n    const posts = ref(\n      getCollection(\"posts\", [\"userId\", \"==\", route.params.id]).documents\n    );\n    const grid = computed(() => {\n      return {\n        posts: posts.value,\n      };\n    });\n\n    return { user, userInfo, route, posts, grid };\n  },\n};\n</script>\n\n<style></style>\n"],"sourceRoot":""}]}