        <div class=\"thumbnail\">\n          <img :src=\"post.coverUrl\">\n        </div>\n        <div class=\"info\">\n          <h4>{{ post.title }}</h4>\n          <p class=\"mt-1\">{{descriptionSnippet}}</p>\n        </div>\n        <div class=\"post-author pb-1 d-flex\">\n          <div class=\"d-flex mr-4\">\n           <img class=\"pr-1\" src=\"@/assets/icons/person.png\" alt=\"person\">\n           <p> {{ post.userName }}</p>\n          </div>\n           <div class=\"d-flex mr-4\">\n           <img class=\"pr-1\" src=\"@/assets/icons/date.png\" alt=\"person\">\n           <p>today</p>\n          </div>\n         \n          \n        </div>\n      </div>\n      </router-link>\n      </div>\n</template>\n\n<script>\nimport moment from \"moment\"\nimport { computed } from '@vue/runtime-core'\nexport default {\n    props: ['post'],\n\nsetup(props) { \n    // computing length of description output\n    const descriptionSnippet = computed(() => {\n        return props.post.description.substring(0,90) + '....'\n    }) \n    const formatedTime = computed(() => {\n        return  moment(props.post.createdAt).format('lll')\n    }) \n    return { descriptionSnippet, formatedTime }\n}\n}\n</script>\n\n<style scoped>\n    .single {\n    display: flex;\n    flex-direction: column;\n    padding: 0;\n    border-radius: 10px;\n    background: white;\n    margin: 16px 0;\n    transition: all ease 0.2s;\n    \n  }\n  .single:hover {\n    box-shadow: 1px 2px 3px rgba(50,50,50,0.05);\n    transform: scale(1.02);\n    transition: all ease 0.2s;\n  }\n  .thumbnail {\n    width: 100%;\n    max-height: 300px;\n    overflow: hidden;\n    border-radius: 10px;\n    min-height: 100%;\n  }\n  img {\n    max-width: 150%;\n    max-height: 150%;\n    display: block;\n  }\n  .info {\n    margin: 10px 20px;\n    height: 10rem;\n  }\n  .post-author {\n    margin: 10px 20px;\n  }\n</style>"]}]}